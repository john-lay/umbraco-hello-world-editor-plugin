@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.HelloWorldPage>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "Master.cshtml";
    ViewBag.Title = CurrentPage.Name;
}
<div class="container" id="hello-world-container">
    @CurrentPage.GetGridHtml(Html, "content") @* the second parameter is the document type alias (tab name) *@
</div>
